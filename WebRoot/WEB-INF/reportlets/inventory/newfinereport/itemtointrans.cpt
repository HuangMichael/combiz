<WorkBook xmlVersion="20091224"><TableDataMap><TableData name="invitemlist" class="com.fr.data.impl.DBTableData"><Parameters><Parameter><Attributes name="warehouse"/><O><![CDATA[]]></O></Parameter></Parameters><Attributes maxMemRowCount="-1"/><Connection class="com.fr.data.impl.NameDatabaseConnection"><DatabaseName><![CDATA[inbasis]]></DatabaseName></Connection><Query><![CDATA[SELECT i.itemnum,i.description,i.rotating,i.modelnum,i.classid,y.warehouse FROM item i LEFT JOIN inventory y ON y.itemnum=i.itemnum WHERE i.status='在用' AND y.warehouse='[?warehouse|?]A' ORDER BY y.warehouse,i.classid DESC]]></Query></TableData><TableData name="rectity" class="com.fr.data.impl.DBTableData"><Parameters><Parameter><Attributes name="year"/><O><![CDATA[]]></O></Parameter><Parameter><Attributes name="month"/><O><![CDATA[]]></O></Parameter></Parameters><Attributes maxMemRowCount="-1"/><Connection class="com.fr.data.impl.NameDatabaseConnection"><DatabaseName><![CDATA[inbasis]]></DatabaseName></Connection><Query><![CDATA[SELECT r.towarehouse,r.itemnum,r.transdate, SUM(r.quantity) as rectity,SUM(r.linecost) as reclinecost FROM invrectrans r WHERE r.rectype='接收' AND r.status='已检验' AND substr(to_char(r.transdate,'yyyy-mm-dd'),1,7) = '[?year|?]A'||'-'||'[?month|?]A' GROUP BY r.towarehouse,r.itemnum,r.transdate]]></Query></TableData><TableData name="usetity" class="com.fr.data.impl.DBTableData"><Parameters><Parameter><Attributes name="year"/><O><![CDATA[2008]]></O></Parameter><Parameter><Attributes name="month"/><O><![CDATA[10]]></O></Parameter></Parameters><Attributes maxMemRowCount="-1"/><Connection class="com.fr.data.impl.NameDatabaseConnection"><DatabaseName><![CDATA[inbasis]]></DatabaseName></Connection><Query><![CDATA[SELECT u.warehouse,u.itemnum,u.transdate, SUM(u.quantity) as usetity,SUM(u.linecost) as usecost FROM invusetrans u WHERE u.state='已完成' AND u.issuetype='退回' AND substr(to_char(u.transdate,'yyyy-mm-dd'),1,7) = '[?year|?]A'||'-'||'[?month|?]A' GROUP BY u.warehouse,u.itemnum,u.transdate]]></Query></TableData><TableData name="sendtity" class="com.fr.data.impl.DBTableData"><Parameters><Parameter><Attributes name="year"/><O><![CDATA[]]></O></Parameter><Parameter><Attributes name="month"/><O><![CDATA[]]></O></Parameter></Parameters><Attributes maxMemRowCount="-1"/><Connection class="com.fr.data.impl.NameDatabaseConnection"><DatabaseName><![CDATA[inbasis]]></DatabaseName></Connection><Query><![CDATA[SELECT s.warehouse,s.itemnum,s.transdate, SUM(s.quantity) as sendtity,SUM(s.linecost) as sendcost FROM invusetrans s WHERE s.state='已完成' AND s.issuetype='发放' AND substr(to_char(s.transdate,'yyyy-mm-dd'),1,7) = '[?year|?]A'||'-'||'[?month|?]A' GROUP BY s.warehouse,s.itemnum,s.transdate]]></Query></TableData><TableData name="retentity" class="com.fr.data.impl.DBTableData"><Parameters><Parameter><Attributes name="year"/><O><![CDATA[]]></O></Parameter><Parameter><Attributes name="month"/><O><![CDATA[]]></O></Parameter></Parameters><Attributes maxMemRowCount="-1"/><Connection class="com.fr.data.impl.NameDatabaseConnection"><DatabaseName><![CDATA[inbasis]]></DatabaseName></Connection><Query><![CDATA[SELECT t.towarehouse,t.itemnum,t.transdate, SUM(t.quantity) as retentity,SUM(t.linecost) as reccost FROM invrectrans t WHERE t.rectype='退回' AND t.status='已检验' AND substr(to_char(t.transdate,'yyyy-mm-dd'),1,7) = '[?year|?]A'||'-'||'[?month|?]A' GROUP BY t.towarehouse,t.itemnum,t.transdate]]></Query></TableData><TableData name="transtity" class="com.fr.data.impl.DBTableData"><Parameters><Parameter><Attributes name="year"/><O><![CDATA[2008]]></O></Parameter><Parameter><Attributes name="month"/><O><![CDATA[10]]></O></Parameter></Parameters><Attributes maxMemRowCount="-1"/><Connection class="com.fr.data.impl.NameDatabaseConnection"><DatabaseName><![CDATA[inbasis]]></DatabaseName></Connection><Query><![CDATA[SELECT a.warehouse,a.itemnum,a.transdate, SUM(a.quantity) as transtity,SUM(a.linecost) as transcost FROM invtrans a WHERE substr(to_char(a.transdate,'yyyy-mm-dd'),1,7) = '[?year|?]A'||'-'||'[?month|?]A' GROUP BY a.warehouse,a.itemnum,a.transdate]]></Query></TableData><TableData name="curbal" class="com.fr.data.impl.DBTableData"><Parameters/><Attributes maxMemRowCount="-1"/><Connection class="com.fr.data.impl.NameDatabaseConnection"><DatabaseName><![CDATA[inbasis]]></DatabaseName></Connection><Query><![CDATA[SELECT n.warehouse,n.itemnum,SUM(n.curbal) as curbal,SUM(n.curbal*i.avgcost) as linecost FROM invstock n LEFT JOIN inventory i ON i.warehouse=n.warehouse AND i.itemnum=n.itemnum GROUP BY n.warehouse,n.itemnum]]></Query></TableData><TableData name="lastmonqty" class="com.fr.data.impl.DBTableData"><Parameters><Parameter><Attributes name="year"/><O><![CDATA[2008]]></O></Parameter><Parameter><Attributes name="month"/><O><![CDATA[10]]></O></Parameter></Parameters><Attributes maxMemRowCount="-1"/><Connection class="com.fr.data.impl.NameDatabaseConnection"><DatabaseName><![CDATA[inbasis]]></DatabaseName></Connection><Query><![CDATA[SELECT o.warehouse,o.itemnum,o.lastmonqty,o.lastmoncost FROM invreport o WHERE substr(to_char(o.thisdate,'yyyy-mm-dd'),1,7) = '[?year|?]A'||'-'||'[?month|?]A']]></Query></TableData></TableDataMap><ReportParameterAttr><Attributes showWindow="false" delayPlaying="true" windowPosition="0" align="1"/><PWTitle><![CDATA[参数]]></PWTitle></ReportParameterAttr><Report class="com.fr.report.WorkSheet" name="sheet1"><ReportSettings orientation="1" shrinkToFitMode="0" headerHeight="0.33" footerHeight="0.33"><PaperSize width="8.504212598425198" height="11.88984251968504"/><Margin top="0.27" left="0.7499999999999999" bottom="0.27" right="0.7499999999999999"/></ReportSettings><ReportWebAttr><EmailManager mailHost="" password="___" user="" fromEmailAddres=""/><ServerPrinter/><Background name="NullBackground"/></ReportWebAttr><ReportWriteAttr/><RowHeight defaultValue="19"><![CDATA[27,20,19,19,19,19,19,19,19,19,19]]></RowHeight><ColumnWidth defaultValue="72"><![CDATA[72,72,98,86,72,72,72,72,72,72,72,72,72,72,72,65,