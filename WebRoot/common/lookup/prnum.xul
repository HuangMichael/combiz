<?xml version="1.0" encoding="GBK"?>
<!-- 获取LOOKUP结果集 -->
<lookupwindow title="采购申请" width="600">
	<zscript><![CDATA[
//设置默认过滤
String	queryString="";
lookupWnd.setQueryString(queryString);
lookupWnd.setOrderby("");
]]></zscript>

	<!-- //////////////////查询过滤条件///////////////////////// -->
	<groupbox id="searchBox">
		<caption label="过滤器" />
		采购申请编号：
		<stextbox data="prnum" cols="10" />
		供应商:
		<stextbox data="vendor" cols="10" />
		申请日期：
		<ssdatebox data="requestdate" cols="10" />
		  至  
		<sedatebox data="requestdate" cols="10" />
		<image src="/images/button/search.gif" style="cursor:hand" onClick="lookupWnd.search()" />
	</groupbox>
	<!-- //////////////////查询过滤条件///////////////////////// -->

	<!-- //////////////////数据列表区///////////////////////// -->
	<pagenavigator />
	<lookuplist pageSize="20">
		<listhead>
			<lookuplistheader data="prnum" label="采购申请号" sort="auto" width="10%" />
			<lookuplistheader data="description" label="描述" sort="auto" width="30%" />
			<lookuplistheader data="requestdept" label="需求部门" sort="auto" width="15%" />
			<lookuplistheader data="vendor" label="供应商" sort="auto" width="10%" />
			<lookuplistheader data="status" label="状态" sort="auto" width="10%" />
			<lookuplistheader data="requestdate" label="申请日期" sort="auto" width="25%" />
		</listhead>
	</lookuplist>
	<!-- //////////////////数据列表区///////////////////////// -->

	<div align="center">
		<button label=" 确 定 " onClick="lookupWnd.ok(event)" />
		<button label=" 取 消 " onClick="lookupWnd.cancel(event)" />
	</div>

</lookupwindow>
